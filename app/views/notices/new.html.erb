<h1>notice#new</h1>

<%= form_for(@notice) do |f| %>
	<%= render "fields", f: f %>
	<%= f.submit "gogo" %>
<% end %>

<script>
	$(document).ready(function() {
		$('#new_notice').validate({
			rules: {
				'notice[question]': { required: true },
				'notice[answer]': { required: true }
			}
		});
	});
</script>
