<%= form_tag(user_mymap_snapshot_path, id: "new_snapshot") do %>
	<%= hidden_field_tag 'lat', '' %>
	<%= hidden_field_tag 'lng', '' %>
	<%= hidden_field_tag 'level', '' %>
	<%= submit_tag 'take a new MyMap snapshot' %>
<% end %>

<script>
	$("#new_snapshot").submit(function(){
		$("#lat").val(oMap.getCenter().getLat());
		$("#lng").val(oMap.getCenter().getLng());
		$("#level").val(oMap.getLevel());
	});
</script>
