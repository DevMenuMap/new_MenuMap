<h1>Pictures#index</h1>

<%= link_to "new picture", new_picture_path %>

<% @pictures.each do |picture| %>
	<br />
	<%= picture.id %> |
	<%= picture.imageable_id %> |
	<%= picture.created_at %>
	<br />
	<%= image_tag picture.img.url %>
	<br />
	<%= link_to "edit", edit_picture_path(picture) %>
	<%= link_to "delete", picture_path(picture), method: :delete %>
	<br />
<% end %>
