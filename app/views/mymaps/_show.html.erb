<table>
	<tr>
		<th>mymap_id</th>
		<th>restaurant_id</th>
		<th>rating</th>
		<th>group</th>
		<th>contents</th>
	</tr>

	<tr>
		<td><%= @mymap.id %></td>
		<td><%= @mymap.restaurant_id %></td>
		<% if !@mymap.rating.blank? && @mymap.rating != 0 %>
			<td class="star_rating" data-score=<%= @mymap.rating%>></td>
		<% else %>
			<td></td>
		<% end %>
		<td><%= @mymap.group %></td>
		<td><%= @mymap.contents %></td>
	</tr>

	<tr>
		<td><%= link_to "edit_mymap", edit_mymap_path(@mymap), { id: "edit_mymap_button", remote: true } %></td>
		<td><%= link_to "destroy_mymap", mymap_path(@mymap), { id: "destroy_mymap_button", method: :delete, remote: true } %></td>
	</tr>
</table>

<script>
	$(".star_rating").raty({
		path: "<%= s3_image_url %>",
		readOnly: true,
		score: function() {
			return parseInt($(this).attr("data-score")) / 2;
		}
	});
</script>
