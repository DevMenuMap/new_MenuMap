<tr id="menu_<%= menu.id %>">
	<td style="width: 100px;"><%= menu.name %></td>
	<td style="width: 160px;"><%= menu.side_info %></td>
	<td style="width: 80px;"><%= menu.price %></td>
	<td style="width: 600px;"><%= menu.info %></td>
	<td style="width: 20px;"><%= menu.sitga %></td>
	<td><%= link_to "edit", edit_menu_path(menu), class: "edit_menu", remote: true %>
	<td><%= link_to "delete", menu_path(menu), class: "delete_menu", method: :delete, remote: true, data: { confirm: "gogo?" } %>
</tr>
